#ifndef __SELCO__
#define __SELCO__

typedef double R;

void selco_paroi(R epsi, int & nbcop,int ** CONTP, R * DCONTP, R ** NCONTP, int NB_PAR, R ** LIST_PX, R ** LIST_PY, R ** LIST_PZ, R ** LIST_PN, int NB_SPH, R * LIST_R, R * LIST_X, R * LIST_Y, R * LIST_Z);
